{% extends 'base.html.twig' %}

{% block body %}
    
    <div id="home">
        <img class="displayed" src="{{ asset('img/logo.png') }}" />
        <div id="main-form">
            <!-- Begin MailChimp Signup Form -->
            <div id="mc_embed_signup">
                <form action="https://unblog.us8.list-manage.com/subscribe/post?u=401a984db528b45ce25452792&amp;id=eb2e374160" method="post" id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate" target="_blank" novalidate>
                    <div id="mc_embed_signup_scroll">
                        <div class="mc-field-group">
                            <label for="mce-EMAIL">Email </label>
